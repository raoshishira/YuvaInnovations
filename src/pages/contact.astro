---
import ContactForm from "@/components/forms/Contact.astro";
import BaseLayout from "@/layouts/BaseLayout.astro";
import type { AstroSeoProps } from "@astrolib/seo";

const seo: AstroSeoProps = {
	title: "Contact Yuva Innovations: Get in Touch for Innovative Full Stack Marketing Solutions",
	description:
		"Reach out to Yuva Innovations for expert Marketing services. Connect with us to discuss your project and explore cutting-edge solutions.",
	canonical: "https://yuvainnovations.in/contact",
};
---

<BaseLayout seo={seo}>
	<ContactForm />
</BaseLayout>
